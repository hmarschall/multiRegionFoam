FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default					backward;
}

gradSchemes
{
    default         		leastSquares;
}

divSchemes
{
    default         		none;
    div(phi,T)              Gauss linearUpwind Gauss linear;
}

laplacianSchemes
{
    default         		none;
    laplacian(alpha,T)      Gauss linear corrected;
}

interpolationSchemes
{
    default         		linear;
}

snGradSchemes
{
    default         		corrected;
}

fluxRequired
{
    default         no;
    T;
}

