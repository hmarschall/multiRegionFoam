FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/InnerFluid";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    interface
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 101325;
    }
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    slip-bottom
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}


