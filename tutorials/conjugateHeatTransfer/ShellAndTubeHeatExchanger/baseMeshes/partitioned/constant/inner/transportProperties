FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          transportProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


// Density - Dimensions: kg/m3
rho             rho [1 -3 0 0 0 0 0] 1027; 

mu              mu [ 1 -1 -1 0 0 0 0 ] 3.645e-4; 

// Specific heat - Dimensions: J/kg-K
cp              cp  [0 2 -2 -1 0 0 0] 4195; 


// Thermal conductivity - Dimensions: W/m-K
k            	k   [1 1 -3 -1 0 0 0] 0.668; //kf=cp*mu/Pr = 4195*3.645e-4/2.289

// Jump coefficient
K               K [ 0 0 0 0 0 0 0 ] 1;

