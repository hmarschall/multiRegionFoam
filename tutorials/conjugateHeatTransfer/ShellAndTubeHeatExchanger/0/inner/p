FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/InnerFluid";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    outlet
    {
        type            fixedValue; 
        value           $internalField; 
    }
    inlet
    {
        type            zeroGradient;
    }
    inner_to_solid
    {
        type            zeroGradient;
    }
    inner_to_domain1
    {
        type            zeroGradient;
    }
}


