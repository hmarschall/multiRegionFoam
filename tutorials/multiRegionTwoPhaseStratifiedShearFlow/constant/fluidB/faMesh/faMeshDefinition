FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/faMesh";
    object      faMeshDefinition;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

polyMeshPatches  1( interfaceShadow );

boundary
{
    outlet
    {
        type                patch;
        ownerPolyPatch      interfaceShadow;
        neighbourPolyPatch  outlet;
    }

    inlet
    {
        type                patch;
        ownerPolyPatch      interfaceShadow;
        neighbourPolyPatch  inlet;
    }

    frontAndBack
    {
        type                empty;
        ownerPolyPatch      interfaceShadow;
        neighbourPolyPatch  frontAndBack;
    }
}


