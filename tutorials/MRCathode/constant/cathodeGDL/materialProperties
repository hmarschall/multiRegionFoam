// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


// Porosity - Dimensions: -
epsilonP       	epsilonP [0 0 0 0 0 0 0] 0.76;

// Tortuosity - Dimensions: -
tau            	tau  [0 0 0 0 0 0 0] 1.6;

// layer thickness - Dimensions: m
d		d  [0 1 0 0 0 0 0] 1.6e-4;

// molecular weight water - Dimensions: kg/mol
MW		MW  [1 0 0 0 -1 0 0] 0.018;

// molar volume of liquid water - Dimensions: mÂ³/mol
VW		VW  [0 3 0 0 -1 0 0] 1.841e-5;

// irreducible saturation - Dimensions: -
sIm		sIm  [0 0 0 0 0 0 0] 0.12;

// liquid gas exchange area - Dimensions: 1/m
aLG		aLG  [0 -1 0 0 0 0 0] 2e6;


