FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      coupledSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regions
(
    // ( fluidA (icoFluid) )
    // ( fluidB (icoFluid) )
    ( fluidA (interTrackFluid) )
    ( fluidB (interTrackFluid) )
);

// Assembles a super-mesh
superMeshRegions
();

DNA
{
    pUPimple
    {
        maxCoupleIter 10;

        residualControl
        {
            U
            {
                maxJumpRes 1e-5;
                outputJumpResField yes;
                maxFluxRes 1e-4;
                outputFluxResField yes;
            }

            p
            {
                maxJumpRes 1e-5;
                outputJumpResField yes;
                maxFluxRes 1e-5;
                outputFluxResField yes;
            }
        }
    }
}

