    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           $internalField;
    }

    airSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }

    airInletSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }

    airOutletSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }

    cathodeSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }    
/*    cflSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }
*/        
    air_to_interconnect
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    air_to_electrolyte //cathode
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
