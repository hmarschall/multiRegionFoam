FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          yH2;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.75226880740;

boundaryField
{
    fuelInlet
    {
        type            fixedValue;
        value           $internalField;
    }

    fuelOutlet
    {
//         type            zeroGradient;
        type            inletOutlet;
        phi             phi;
        value           $internalField;
        inletValue      $internalField;
    }

    fuelSides
    {
        type            zeroGradient;
    }

    fuelInletSides
    {
        type            zeroGradient;
    }

    fuelOutletSides
    {
        type            zeroGradient;
    }
    
    anodeSides
    {
        type            zeroGradient;
    }
    
    aflSides
    {
        type            zeroGradient;
    }
    
    fuel_to_electrolyte //anode
    {
        type            fixedGradient;
        gradient        uniform 0;
    }

    fuel_to_interconnect
    {
        type            zeroGradient;
    }
}


