FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 363.15;

boundaryField
{
    electrolyteSides
    {
        type            zeroGradient;
    }
    electrolyte_to_air
    {
        type                    regionCoupledHeatFlux;
        interfaceType           heatTransferInterface;
        k                       "k";

        accType               fixed;
        relax                   0.01;

        neighbourRegionName     air;
        neighbourPatchName      air_to_electrolyte;
        neighbourFieldName      T;
        secondOrder             false;
        nonOrthCorr             true;
        value                   $internalField;
    }
    electrolyte_to_fuel
    {
        type                    regionCoupledHeatFlux;
        interfaceType           heatTransferInterface;
        k                       "k";

        accType               fixed;
        relax                   0.01;

        neighbourRegionName     fuel;
        neighbourPatchName      fuel_to_electrolyte;
        neighbourFieldName      T;
        secondOrder             false;
        nonOrthCorr             true;
        value                   $internalField;
//         type            zeroGradient;
//         type                    fixedValue;
//         value                   uniform 373.15;
    }
}


