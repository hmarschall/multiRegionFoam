FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type    singlePhaseSystem;


fuel
{
    type            surfaceReactingFickPhaseModel;
    diameterModel   isothermal;
    isothermalCoeffs
    {
        d0              3e-3;
        p0              1e5;
    }
    Sc              0.7;

    residualAlpha   1e-6;
}

phaseNames
(
    // iter.key() (dummy here)       // iter()
        phase1                      (fuel) 
);

// Surface electrochemistry
// surface electrochemistry using the boundary condittions for Yi and u
surfaceElectrochemistry true;

fluidCatalystName   fuel_to_electrolyte;
ionCatalystName   phiI_to_phiE0;
electronCatalystName   phiEA_to_phiI0;


//- ElectrochemicalReactingPhaseModel

catalystZoneName acl;

regions
{
    fluid
    {
        name    fuel;
    }
    electron
    {
        name    phiEA;
    }
    ion
    {
        name    phiI;
    }
    dissolved
    {
        name    dissolved;
    }
}

saturation
{
    type ArdenBuck;
}

species
{
    H2
    {
        ksi     1.0;
        cRef    26.3;
    }
}

// Butler Volmer

phiNames
(
    electron    phi
    ion         phi
);

iNames
(
    electron    i
    ion         i
);

i0              2000;
relax           0.5;
alpha           0.5;
gamma           1.5;

Tref            353.15;
EA              145000;           // For Nickel and Oxygen [J/mol]  -> variieren sehr stark, aber ein hÃ¶herer Wert bedeutet ein exponentiell ansteigenden Wert in i0


// Nernst

pRef        101325;

RxnList
(
    H2     -1
    e      2
);


// Minimum allowable pressure
pMin            10000;

