FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          porousZones;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// ( without "(" and ")"

anode
{
    type            DarcyForchheimer;	// porosity Model: fixCoeff, powerLaw, DarcyForchheimer
    active          yes;		// status
    cellZone        anode;		// name of porous zone
    porosity        0.7;		// porosity

    DarcyForchheimerCoeffs		// coefficents for DarcyForchheimer; with the name of type + Coeffs
    {
        d   (1e11 1e11 1e11);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);
            coordinateRotation
            {
                type    axesRotation;
                e1      (1 0 0);    //(0.70710678 0.70710678 0);
                e2      (0 0 1);
            }
        }
    }

        Cp 568;
        k  1.7;

// ------------------------ diffusivity model ----------------------------//
        diffusivity
        {
//            type        fixedDiffusivity;
//            diff0       diff0 [0 2 -1 0 0 0 0]  5e-5;
            // ------------------------------------------ //
            type        porousFSG;
            Tname       T.fuel;
            pName       p_rgh;
            alphaName   alpha.fuel;
            speciesA    H2;     // diffusing species
            speciesB    H2O;    // background species
            porosity    0.7;
            tortuosity  1.15;
            dPore       dPore [0 1 0 0 0 0 0] 1.333333e-06;   // pore size
            doBinary    false;
        }
}

mpl
{
    type            DarcyForchheimer;	// porosity Model: fixCoeff, powerLaw, DarcyForchheimer
    active          yes;		// status
    cellZone        mpl;		// name of porous zone
    porosity        0.4;		// porosity

    DarcyForchheimerCoeffs		// coefficents for DarcyForchheimer; with the name of type + Coeffs
    {
        d   (1e12 1e12 1e12);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);
            coordinateRotation
            {
                type    axesRotation;
                e1      (1 0 0);    //(0.70710678 0.70710678 0);
                e2      (0 0 1);
            }
        }
    }

	Cp 3300;
	k 1;

// ------------------------ diffusivity model ----------------------------//
        diffusivity
        {
//            type        fixedDiffusivity;
//            diff0       diff0 [0 2 -1 0 0 0 0]  5e-5;
            // ------------------------------------------ //
            type        porousFSG;
            Tname       T.fuel;
            pName       p_rgh;
            alphaName   alpha.fuel;
            speciesA    H2;     // diffusing species
            speciesB    H2O;    // background species
            porosity    0.4;
            tortuosity  1.6;
            dPore       dPore [0 1 0 0 0 0 0] 1.333333e-06;   // pore size
            doBinary    false;
        }
}
