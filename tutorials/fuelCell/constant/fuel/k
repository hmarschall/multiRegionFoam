FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];

internalField   uniform 1;

boundaryField
{
    anodeSides
    {
        type            zeroGradient;
    }
    fuelInlet
    {
        type            fixedValue;
        value           uniform 1e-15;
    }
    fuelOutlet
    {
        type            zeroGradient;
    }
    fuel_to_electrolyte
    {
        type            calculated;
        value           uniform 0;
    }
    fuel_to_interconnect
    {
        type            calculated;
        value           uniform 0;
    }
}


