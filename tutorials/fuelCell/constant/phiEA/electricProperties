//- Add information here

sigma
{
    bpp
    {
        sigmaModel      constantSigma;

        constantSigmaCoeffs
        {
            cellZone    bpp;
            sigma       8000;
        }
    }

    ampl
    {
        sigmaModel      porousSigma;

        porousSigmaCoeffs
        {
            cellZone        ampl;
            catalyst        no;
            porosity        0.4;
            sigma           8000;
        }
    }

    anode
    {
        sigmaModel      porousSigma;

        porousSigmaCoeffs
        {
            cellZone        anode;
            catalyst        no;
            porosity        0.7;
            sigma           8000;
        }
    }
}
