FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          porousZones;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

cathode
{
    type            DarcyForchheimer;	// porosity Model: fixCoeff, powerLaw, DarcyForchheimer
    active          yes;		// status
    cellZone        cathode;		// name of porous zone
    porosity        0.7;		// porosity

    DarcyForchheimerCoeffs		// coefficents for DarcyForchheimer; with the name of type + Coeffs
    {
        d   (1e11 1e11 1e11);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);
            coordinateRotation
            {
                type    axesRotation;
                e1      (1 0 0);    //(0.70710678 0.70710678 0);
                e2      (0 0 1);
            }
        }
        theta 2.111848395;
        sigma 0.0625;
    }

    Cp 568;
    k  1;

// ------------------------ diffusivity model ----------------------------//
      diffusivity
      {
            type        porousFSG;
            Tname       T.air;
            pName       p;
            alphaName   alpha.air;
            speciesA    N2;     // diffusing species
            speciesB    O2;     // background species
            porosity    0.7;
            tortuosity  1.15;
            dPore       dPore [0 1 0 0 0 0 0] 16.5e-06;   // pore size
            doBinary    false;
      }
}

mpl
{
    type            DarcyForchheimer;	// porosity Model: fixCoeff, powerLaw, DarcyForchheimer
    active          yes;		// status
    cellZone        mpl;		// name of porous zone
    porosity        0.4;		// porosity

    DarcyForchheimerCoeffs		// coefficents for DarcyForchheimer; with the name of type + Coeffs
    {
        d   (1e12 1e12 1e12);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);
            coordinateRotation
            {
                type    axesRotation;
                e1      (1 0 0);    //(0.70710678 0.70710678 0);
                e2      (0 0 1);
            }
        }
        theta 2.111848395;
        sigma 0.0625;
    }

	Cp 568;
	k 1;

// ------------------------ diffusivity model ----------------------------//
      diffusivity
      {
            type        porousFSG;
            Tname       T.air;
            pName       p;
            alphaName   alpha.air;
            speciesA    N2;     // diffusing species
            speciesB    O2;     // background species
            porosity    0.4;
            tortuosity  1.6;
            dPore       dPore [0 1 0 0 0 0 0] 46e-06;   // pore size
            doBinary    false;
      }
}

