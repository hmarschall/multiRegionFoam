FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1/phiEC";
    object      T2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 333;

boundaryField
{
    interconnect0
    {
        type            fixedValue;
        value           uniform 333;
    }
    interconnect0Sides
    {
        type            zeroGradient;
    }
    cathodeSides
    {
        type            zeroGradient;
    }
    phiEC_to_phiI0
    {
        type            calculated;
        value           uniform 0;
    }
    phiEC_to_phi0
    {
        type            calculated;
        value           uniform 0;
    }
}


