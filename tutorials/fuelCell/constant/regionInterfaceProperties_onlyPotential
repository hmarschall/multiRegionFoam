(
    phiEAphiI
    {
        interfaceType currentTransferInterface;

        coupledPatchPair
        (   // region patch
            (phiEA phiEA_to_phiI0)
            (phiI phiI_to_phiE0)
        );

        coupledFields
        (
            phi
        );
        
        currentTransferInterfaceCoeffs
        {
            fluidPhaseName fuel;
            fluidPatchName  fuel_to_electrolyte;
            etaSign -1;
        }
    }
    
    phiECphiI
    {
        interfaceType currentTransferInterface;

        coupledPatchPair
        (   // region patch
            (phiEC phiEC_to_phiI0)
            (phiI phiI_to_phiE1)
        );

        coupledFields
        (
            phi
        );
        
        currentTransferInterfaceCoeffs
        {
            fluidPhaseName air;
            fluidPatchName  air_to_electrolyte;
            etaSign 1;
        }
    }
);

