FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 434.15;

boundaryField
{
    electrolyteSides
    {
        type            zeroGradient;
    }
    electrolyte_to_air
    {
        type            calculated;
        value           uniform 0;
    }
    electrolyte_to_fuel
    {
        type            calculated;
        value           uniform 0;
    }
}


