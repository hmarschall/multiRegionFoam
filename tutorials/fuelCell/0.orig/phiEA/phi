FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          phi;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0; // uniform (0.566711 0 0);

boundaryField
{

    interconnect1
    {
        type            fixedValue;
		value			uniform 0;
    }

	phiEA_to_phi0
    {
        type            zeroGradient;
    }
    
	phiEA_to_phiI0
    {
       type                     regionCoupledPotentialFlux;
       interfaceType           currentTransferInterface;
       k              "sigma";                   // k is sigma here, needed in both, the jumpBC and the flux BC
       
       neighbourRegionName      phiI;
       neighbourPatchName       phiI_to_phiE0;
       neighbourFieldName       phi;
       secondOrder             false;
       nonOrthCorr             true;
       value                    $internalField;
    }
    
    phiEA_to_phi0
    {
        type            zeroGradient;
    }
    
    interconnectSides
    {
        type            zeroGradient;
    }
    
	cathodeSides
    {
        type            zeroGradient;
    }
    
	anodeSides
    {
        type            zeroGradient;
    }
    
	phiEA_to_phi
    {
        type            zeroGradient;
    }
}


// ************************************************************************* //
