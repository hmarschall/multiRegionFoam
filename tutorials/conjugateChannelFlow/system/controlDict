FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application 		MRconjugateHeatFoam;
startFrom       	startTime;
startTime       	0;
stopAt          	endTime;
endTime         	10.0;
deltaT          	1E-2;
writeControl    	timeStep;
writeInterval   	10;
purgeWrite      	0;
writeFormat     	ascii;
writePrecision  	8;
writeCompression 	uncompressed;
timeFormat      	general;
timePrecision   	10;
runTimeModifiable 	yes;


