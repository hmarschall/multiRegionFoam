functions
(
    forces
    {
        type                forces;
        region              fluid;
        functionObjectLibs  ( "libforces.so" );
        outputControl       timeStep;
        outputInterval      1;
        patches             (interfaceShadow);
        pName               pKin;
        UName               U;
        rhoName             rhoInf;
        log                 true;
        rhoInf              1000;
        CofR                (0.25 0.2 0.0075);
    }
    displacement
    {
        type                solidPointDisplacement;
        region              solid;
        functionObjectLibs  ( "libinterfaceTrackingFOs.so" );
        point               (0.6 0.2 0.0075);
    }
);
