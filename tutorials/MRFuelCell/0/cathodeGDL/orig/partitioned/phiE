FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          phiE;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 1.0;

boundaryField
{
    topAndBottom
    {
        type                    empty;
    }
    left
    {
        type                    regionCoupledScalarJump;
        k			"sigma";
        relax			0.1;
       	neighbourRegionName     cathodeCL;
       	neighbourPatchName      right;
       	neighbourFieldName      phiE;
	secondOrder		false;
	nonOrthCorr		true;
       	value                   $internalField;
    }
    right
    {
	type			fixedValue;
	value			uniform 1.0;
        //type                    timeVaryingUniformFixedValue;
	//fileName		"potentialRamp";
        //outOfBounds		clamp;
				
    }
    frontAndBack
    {
        type            	empty;
    }
}

