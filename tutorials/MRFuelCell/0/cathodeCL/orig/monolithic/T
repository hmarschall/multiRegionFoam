FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          T;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 0 1 0 0 0];

internalField   uniform 343;

boundaryField
{
    topAndBottom
    {
        type                    empty;
    }
    left
    {
        type                    regionCoupleScalarFlux;
        k			k;
       	neighbourRegionName     membrane;
       	neighbourPatchName      right;
       	neighbourFieldName      T;
	secondOrder		false;
	nonOrthCorr		true;
       	value                   $internalField;
    }
    right
    {
        type                    regionCoupleScalarJump;
        k			k;
       	neighbourRegionName     cathodeCL;
       	neighbourPatchName      left;
       	neighbourFieldName      T;
	secondOrder		false;
	nonOrthCorr		true;
       	value                   $internalField;
    }
    frontAndBack
    {
        type            	empty;
    }
}

