FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          xV;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.19;

boundaryField
{
    topAndBottom
    {
        type                    empty;
    }
    left
    {
        type                    regionCoupledScalarJump;
        k			"DEffV";
        relax			0.01;
       	neighbourRegionName     anodeGDL;
       	neighbourPatchName      right;
       	neighbourFieldName      xV;
	secondOrder		false;
	nonOrthCorr		true;
       	value                   $internalField;
    }
    right
    {
        type			zeroGradient;
    }
    frontAndBack
    {
        type            	empty;
    }
}

