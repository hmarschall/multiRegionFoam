FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default		Euler;
}

gradSchemes
{
    default         	cellMDLimited Gauss linear 1;
}

divSchemes
{
    default       	Gauss linear;
}

laplacianSchemes
{
    default       	Gauss linear limited 0.333;
}

interpolationSchemes
{
    default         	linear;
}

snGradSchemes
{
    default        	limited 0.333;
}

fluxRequired
{
    default        	no;
    T;
    phiE;
    xH2;
    xV;
}

