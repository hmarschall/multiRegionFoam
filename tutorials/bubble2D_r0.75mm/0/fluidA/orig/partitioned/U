FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    space
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0 0 0);
    }
    interface
    {
        type                    genericInterfaceCoupledVelocityFlux;
        k                       "mu";
        neighbourRegionName     fluidB;
        neighbourPatchName      interfaceShadow;
        neighbourFieldName      U;
        interfaceType           capillaryInterface;
        secondOrder             false;
        nonOrthCorr             false;
        value                   uniform (0 0 0);
    }
    frontAndBackPlanes
    {
        type            empty;
    }
}


