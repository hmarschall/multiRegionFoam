FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
"proc.*"
{
	type	processor;
	value	uniform 0;
}
    interfaceShadow
    {
        type                    interfaceCoupledPressureFlux;
        k                       "rho";
        neighbourRegionName     fluidA;
        neighbourPatchName      interface;
        neighbourFieldName      p;
        secondOrder             yes;
        nonOrthCorr             yes;
        value                   uniform 0;
    }
    centerline
    {
        type            symmetryPlane;
    }
    upperWedge
    {
        type            wedge;
    }
    lowerWedge
    {
        type            wedge;
    }
}


