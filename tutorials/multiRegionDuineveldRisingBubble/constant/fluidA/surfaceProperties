bulkDiffusion       bulkDiffusion       [ 0  2 -1 0  0 0 0 ] 1.0e-9;

//diffusion           diffusion           [ 0  2 -1 0  0 0 0 ] 1.0e-9;
diffusion           diffusion           [ 0  2 -1 0  0 0 0 ] 1.0e-7;

temperature         temperature         [ 0  0  0 1  0 0 0 ] 293.0;
}

