FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      freeSurfaceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceType interface;

normalMotionDir yes;//CAVE!!

motionDir (0 0 0);

cleanInterface yes;

muFluidA muFluidA [ 1 -1 -1 0 0 0 0 ] 1.0e-3;

muFluidB muFluidB [ 1 -1 -1 0 0 0 0 ] 1.82e-5;

rhoFluidA rhoFluidA [ 1 -3  0 0 0 0 0 ] 998.3;

rhoFluidB rhoFluidB [ 1 -3  0 0 0 0 0 ] 1.205;//1.3

cleanSurfaceTension cleanSurfaceTension [ 1 0 -2 0 0 0 0 ] 72.7e-3;

g g [ 0 1 -2 0 0 0 0 ] ( 0 9.81 0 );


fixedSurfacePatches 0 ();

pointNormalsCorrectionPatches 0 ();

nSurfaceCorrectors 1;

relax 0.5;

interfaceDeformationLimit interfaceDeformationLimit [0 0 0 0 0 0 0 ] 1e-6;


surfactantProperties
{
bulkConc            bulkConc            [ 0 -3  0 0  1 0 0 ] 1.0e-2;

saturatedConc       saturatedSurfConc   [ 0 -2  0 0  1 0 0 ] 5.0e-6;

