FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/faMesh";
    object      faMeshDefinition;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

polyMeshPatches  1( interfaceShadow );

boundary
{
    upperWedge
    {
        type                wedge;
        ownerPolyPatch      interfaceShadow;
        neighbourPolyPatch  upperWedge;
    }
    lowerWedge
    {
        type                wedge;
        ownerPolyPatch      interfaceShadow;
        neighbourPolyPatch  lowerWedge;
    }
    centerline
    {
        type                symmetryPlane;
        ownerPolyPatch      interfaceShadow;
        neighbourPolyPatch  centerline;
    }
}

