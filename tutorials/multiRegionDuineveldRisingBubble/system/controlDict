FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs
  (
    "libcoupledFvPatchBCs.so"
  );
  
applicationClass multiRegionFoam;

startFrom latestTime;

startTime 0;

stopAt endTime;

endTime 4.5;

deltaT 1e-4;

writeControl adjustableRunTime; //timeStep;

writeInterval 0.05;//10

cycleWrite 0; 

writeFormat ascii;

writePrecision 16;

writeCompression uncompressed;

timeFormat general;

timePrecision 12;

runTimeModifiable yes;

adjustTimeStep yes;

maxCo 0.1; 

maxDeltaT 1e-04;

minDeltaT 1e-07; 	

