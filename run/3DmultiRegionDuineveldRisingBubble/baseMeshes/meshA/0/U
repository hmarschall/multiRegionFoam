FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 1e-06 0);

boundaryField
{
"proc.*"
{
	type	processor;
	value	uniform (0 0 0);
}
    space
    {
        type            inletOutlet;
        inletValue      uniform (0 1e-06 0);
        value           uniform (0 1e-06 0);
    }
    walls
    {
        type            mixed;
        refValue        uniform (0 1e-06 0);
        refGradient     uniform (0 0 0);
        valueFraction   uniform 1;
        value           uniform (0 1e-06 0);
    }
    interface
    {
        type            interfaceCoupledVelocityFlux;
        neighbourRegionName meshB;
        neighbourPatchName interfaceShadow;
        neighbourFieldName U;
        k               "muFluidA";
        value           uniform (0 0 0);
    }
    centerline
    {
        type            symmetryPlane;
    }
    upperWedge
    {
        type            wedge;
    }
    lowerWedge
    {
        type            wedge;
    }
}


